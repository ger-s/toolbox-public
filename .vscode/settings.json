{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": ["javascript", "javascriptreact"],
  "eslint.workingDirectories": [
    {
      "directory": "react",
      "changeProcessCWD": true
    },
    {
      "directory": "node",
      "changeProcessCWD": true
    }
  ]
}
